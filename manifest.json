{
    "background": {
        "page": "background.html"
    },
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://platform.twitter.com/; object-src 'self'",
    "description": "SVG Screenshot",
    "icons": {
        "128": "icon.png",
        "48" : "icon.png",
        "16" : "icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icon-l.png",
            "38": "icon-l.png"
        },
        "default_title": "SVG Screenshot"
    },
    "manifest_version": 2,
    "name": "SVG Screenshot",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": [
            "lib/jquery-ui.min.css",
            "insite.css"
        ],
        "js": [
            "lib/jquery.js",
            "lib/jquery-ui.min.js",
            "dist/content_script.js"
        ],
        "run_at": "document_end"
    }],

    "web_accessible_resources": [
        "x.png"
    ],

    "permissions": [
        "contextMenus",
        "pageCapture",
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "version": "0.1.3"
}
